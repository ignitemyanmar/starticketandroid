<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBar.Solid.Themestarticket" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_themestarticket</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_themestarticket</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_themestarticket</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Themestarticket</item>
    </style>
    <style name="ActionBar.Transparent.Themestarticket" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_themestarticket</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Themestarticket</item>
    </style>
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        
    </style>
    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar"/>
    <style name="Base.TextAppearance.AppCompat.Button">
        <item name="android:textSize">@dimen/abc_text_size_button_material</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <style name="Platform.AppCompat" parent="Platform.V14.AppCompat"/>
    <style name="Platform.AppCompat.Light" parent="Platform.V14.AppCompat.Light"/>
    <style name="Platform.V14.AppCompat" parent="Platform.V11.AppCompat">
        <item name="android:actionModeSelectAllDrawable">?actionModeSelectAllDrawable</item>

        <item name="android:listPreferredItemPaddingLeft">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:listPreferredItemPaddingRight">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
    <style name="Platform.V14.AppCompat.Light" parent="Platform.V11.AppCompat.Light">
        <item name="android:actionModeSelectAllDrawable">?actionModeSelectAllDrawable</item>

        <item name="android:listPreferredItemPaddingLeft">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:listPreferredItemPaddingRight">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
    <style name="TextAppearance.AppCompat.Notification" parent="@android:style/TextAppearance.StatusBar.EventContent"/>
    <style name="TextAppearance.AppCompat.Notification.Title" parent="@android:style/TextAppearance.StatusBar.EventContent.Title"/>
    <style name="TextAppearance.StatusBar.EventContent" parent="@android:style/TextAppearance.StatusBar.EventContent"/>
    <style name="TextAppearance.StatusBar.EventContent.Info"/>
    <style name="TextAppearance.StatusBar.EventContent.Line2">
        <item name="android:textSize">@dimen/notification_subtext_size</item>
    </style>
    <style name="TextAppearance.StatusBar.EventContent.Time"/>
    <style name="TextAppearance.StatusBar.EventContent.Title" parent="@android:style/TextAppearance.StatusBar.EventContent.Title"/>
    <style name="Theme.Themestarticket" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <!--<item name="windowActionBar">false</item>-->

        <item name="colorPrimary">@color/primary</item> 
                 
        <!-- status bar color -->
        <item name="colorPrimaryDark">@color/primary_dark</item>
        
        <!-- widget color -->
        <item name="colorAccent">@color/accent</item>
        
        <item name="android:actionBarItemBackground">@drawable/selectable_background_themestarticket</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Themestarticket</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Themestarticket</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Themestarticket</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Themestarticket</item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.Themestarticket</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_themestarticket</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_themestarticket</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Themestarticket</item>
        
        
    </style>
    <style name="Theme.Themestarticket.Widget" parent="@style/Theme.AppCompat">
        <item name="android:popupMenuStyle">@style/PopupMenu.Themestarticket</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Themestarticket</item>
    </style>
    <style name="Theme.Themestarticket.userlogin" parent="@style/Theme.Themestarticket">
        <item name="colorAccent">@color/accent</item>
        <item name="android:typeface">serif</item>
    </style>
    <style name="Widget.MaterialProgressBar.ProgressBar" parent="android:Widget.ProgressBar">
        <item name="android:indeterminateDrawable">@null</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:maxWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:maxHeight">48dp</item>
    </style>
    <style name="Widget.MaterialProgressBar.ProgressBar.Horizontal" parent="android:Widget.ProgressBar.Horizontal">
        
        <item name="android:indeterminateDrawable">@null</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:maxHeight">16dp</item>
    </style>
</resources>